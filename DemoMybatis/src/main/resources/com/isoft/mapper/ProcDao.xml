<mapper namespace="com.isoft.dao.ProcDao">
    <!--
    CREATE PROCEDURE proc_news(in v_title varchar(30))
BEGIN
SELECT *
FROM tb_news
WHERE title like v_title
and DATE_FORMAT(pubdatetime,'%Y%m%d')=DATE_FORMAT(NOW(),'%Y%m%d');
END;
CALL proc_news('%');
CREATE FUNCTION fun_news_total(v_title VARCHAR(30))
RETURNS INT
RETURN(
		SELECT COUNT(*)
		FROM tb_news
		WHERE title LIKE v_title

    -->
